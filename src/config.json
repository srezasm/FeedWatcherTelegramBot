{
    "bot-token": "[your bot token]",
    "watch-delay-minutes": 15,
    "log-account-id": "@your_user_name",
    "channels": {
        "[@channel_id]": {
            "formats": {
                "[category name]": "[post format]"
            },
            "feeds": {
                "[feed unique name]": {
                    "name": "[feed display name]",
                    "category": "[feed category]",
                    "tags": ["[first_tag]", "[second_tag]"],
                    "url": "[https://your.feed.url.lol]"
                }
            }
        }
    },
    "logdir": "./logs/",
    "mediadir": "./media/",
    "idsfile": "./ids.txt",
    "etagsfile": "./etags.txt"
}
